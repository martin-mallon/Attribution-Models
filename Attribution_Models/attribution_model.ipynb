    "# fix_timestamp\n",
    "def fix_timestamp(df, col):\n",
    "    ts = df[col].str[6:10] + '-' + df[col].str[3:5] + '-' + df[col].str[:2] + ' ' + df[col].str[11:19]\n",
    "    df['date_time'] = ts.astype('datetime64[ns]')\n",
    "    return df\n",
    "\n",
    "fix_timestamp(att, 'date_time')\n",
    "\n",
    "print(att.dtypes)\n",
    "att.head()"
