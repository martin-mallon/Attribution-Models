    "# melt\n",
    "def melt(df, grp, att_m, att_v, cols, smpl, m):\n",
    "    df = df.reset_index()\n",
    "    df = df.melt(id_vars=[grp], var_name=att_m, value_name=att_v)\n",
    "    df.columns = cols\n",
    "    df[cols[2]] = df[cols[2]]+m\n",
    "    df['sample_ind'] = smpl\n",
    "    return df\n",
    "\n",
    "# variables\n",
    "cols = ['dimension', 'att_model', 'att_value']\n",
    "smpl = heatmap(att.query(sample), 'seq_first_event', mdls, 'event_total', 0)[1]\n",
    "\n",
    "# unpack\n",
    "df_1 = melt(ev_1, 'channel', cols[1], cols[2], cols, 'All Journeys', 0)\n",
    "df_2 = melt(ev_2, 'channel', cols[1], cols[2], cols, 'Multi-Channel Journeys', 0)\n",
    "df_3 = melt(smpl, 'seq_first_event', cols[1], cols[2], cols, 'Theoretical Attribution', 5)\n",
    "\n",
    "# presentation output\n",
    "xl = df_1.append([df_2, df_3])\n",
    "#xl.to_excel('presentation_output_2.xlsx', index=False)\n",
    "xl"
