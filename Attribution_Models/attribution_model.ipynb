    "# heatmap\n",
    "def heatmap(df, grp, mdls, e, n):\n",
    "    \n",
    "    # aggregate by channel\n",
    "    crt = df[e]>=n\n",
    "    piv = df[crt].pivot_table(index=grp,\n",
    "                          values=mdls,\n",
    "                          aggfunc=np.sum)\n",
    "    \n",
    "    # convert Â£ to %\n",
    "    for m in mdls:\n",
    "        ttl = piv[m].sum()\n",
    "        for index, row in piv.iterrows():\n",
    "            row[m] = row[m] / ttl\n",
    "            row[m] = round(row[m]*100,1)\n",
    "    \n",
    "    # heatmap\n",
    "    hmp = piv.style.background_gradient(cmap='Blues')\n",
    "    return hmp, piv\n",
    "\n",
    "# unpack\n",
    "mdls=['att_first_touch', 'att_last_touch', 'att_last_touch_nd', 'att_linear', 'att_decay', 'att_pos']\n",
    "hmp = heatmap(att, 'channel', mdls, 'event_total', 1)\n",
    "hmp[0]"
   ]
